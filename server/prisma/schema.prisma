////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

model DeviceAccounts {
  deviceID String
  accountID String
  version Int
  @@id([deviceID, accountID])
}

model DeviceProfiles {
  deviceID String
  profileID String
  version Int
  @@id([deviceID, profileID])
}

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
